{
  "files.exclude": {
    "**/.git": true,
    "**/dist": true
  },
  "tailwindCSS.experimental.classRegex": [
    ["tv\\((([^()]*|\\([^()]*\\))*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"]
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.enable": true,
  "eslint.validate": ["javascript", "typescript"],
  "editor.formatOnSave": true,
  "eslint.useESLintClass": true
}
